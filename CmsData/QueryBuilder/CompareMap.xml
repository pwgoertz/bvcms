<?xml version="1.0" encoding="utf-8" ?>
<FieldTypes>
  <FieldType Name="Empty" />
  <FieldType Name="Group">
    <Comparison Type="AllTrue" />
    <Comparison Type="AnyTrue" />
    <Comparison Type="AllFalse" />
    <Comparison Type="AnyFalse" />
  </FieldType>
  <FieldType Name="Bit">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="EqualBit">
    <Comparison Type="Equal" Display="{0} = {1}" />
  </FieldType>
  <FieldType Name="NullBit">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="Code">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
    <Comparison Type="OneOf" Display="{0} IN ( {1} )" />
    <Comparison Type="NotOneOf" Display="{0} NOT IN ( {1} )" />
  </FieldType>
  <FieldType Name="NullCode">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
    <Comparison Type="OneOf" Display="{0} IN ({1})" />
    <Comparison Type="NotOneOf" Display="{0} NOT IN ( {1} )" />
  </FieldType>
  <FieldType Name="CodeStr">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
    <Comparison Type="OneOf" Display="{0} IN ( {1} )" />
    <Comparison Type="NotOneOf" Display="{0} NOT IN ( {1} )" />
  </FieldType>
  <FieldType Name="Date">
    <Comparison Type="Equal" Display="{0} = '{1:d}'" />
    <Comparison Type="Greater" Display="{0} > '{1:d}'" />
    <Comparison Type="GreaterEqual" Display="{0} >= '{1:d}'" />
    <Comparison Type="Less" Display="{0} &lt; '{1:d}'" />
    <Comparison Type="LessEqual" Display="{0} &lt;= '{1:d}'" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; '{1:d}'" />
  </FieldType>
  <FieldType Name="DateField">
    <Comparison Type="Equal" Display="{0} = '{1}'" />
    <Comparison Type="Greater" Display="{0} > '{1}'" />
    <Comparison Type="GreaterEqual" Display="{0} >= '{1}'" />
    <Comparison Type="Less" Display="{0} &lt; '{1}'" />
    <Comparison Type="LessEqual" Display="{0} &lt;= '{1}'" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; '{1}'" />
  </FieldType>
  <FieldType Name="Number">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="Greater" Display="{0} > {1}" />
    <Comparison Type="GreaterEqual" Display="{0} >= {1}" />
    <Comparison Type="Less" Display="{0} &lt; {1}" />
    <Comparison Type="LessEqual" Display="{0} &lt;= {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="NumberLG">
    <Comparison Type="Greater" Display="{0} > {1}" />
    <Comparison Type="GreaterEqual" Display="{0} >= {1}" />
    <Comparison Type="Less" Display="{0} &lt; {1}" />
    <Comparison Type="LessEqual" Display="{0} &lt;= {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="NullNumber">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="Greater" Display="{0} > {1}" />
    <Comparison Type="GreaterEqual" Display="{0} >= {1}" />
    <Comparison Type="Less" Display="{0} &lt; {1}" />
    <Comparison Type="LessEqual" Display="{0} &lt;= {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="Integer">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="Greater" Display="{0} > {1}" />
    <Comparison Type="GreaterEqual" Display="{0} >= {1}" />
    <Comparison Type="Less" Display="{0} &lt; {1}" />
    <Comparison Type="LessEqual" Display="{0} &lt;= {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="NullInteger">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="Greater" Display="{0} > {1}" />
    <Comparison Type="GreaterEqual" Display="{0} >= {1}" />
    <Comparison Type="Less" Display="{0} &lt; {1}" />
    <Comparison Type="LessEqual" Display="{0} &lt;= {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="IntegerEqual">
    <Comparison Type="Equal" Display="{0} = {1}" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; {1}" />
  </FieldType>
  <FieldType Name="String">
    <Comparison Type="Equal" Display="{0} = '{1}'" />
    <Comparison Type="StartsWith" Display="{0} = '{1}*'" />
    <Comparison Type="EndsWith" Display="{0} = '*{1}'" />
    <Comparison Type="Contains" Display="{0} = '*{1}*'" />
    <Comparison Type="DoesNotStartWith" Display="{0} &lt;&gt; '{1}*'" />
    <Comparison Type="DoesNotEndWith" Display="{0} &lt;&gt; '*{1}'" />
    <Comparison Type="DoesNotContain" Display="{0} &lt;&gt; '*{1}*'" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; '{1}'" />
    <Comparison Type="After" Display="{0} &gt; '{1}'" />
    <Comparison Type="AfterOrSame" Display="{0} &gt;= '{1}'" />
    <Comparison Type="Before" Display="{0} &lt; '{1}'" />
    <Comparison Type="BeforeOrSame" Display="{0} &lt;= '{1}'" />
  </FieldType>
  <FieldType Name="StringEqual">
    <Comparison Type="Equal" Display="{0} = '{1}'" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; '{1}'" />
  </FieldType>
  <FieldType Name="StringEqualOrStartsWith">
    <Comparison Type="Equal" Display="{0} = '{1}'" />
    <Comparison Type="StartsWith" Display="{0} = '{1}*'" />
    <Comparison Type="NotEqual" Display="{0} &lt;&gt; '{1}'" />
  </FieldType>
</FieldTypes>
