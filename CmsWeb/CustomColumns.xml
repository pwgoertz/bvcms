      op.AttPct 
    </column>
    <column name="OrgMemberType" join="op" context="org"> 
      op.MemberType 
    </column>
    <column name="OrgMemberInactive" join="op" context="org"> 
      op.InactiveDate
    </column>
    <column name="OrgMemberLastAttDt" join="op" context="org"> 
      op.LastAttended
    </column>
    <column name="OrgMemberGrade" join="op" context="org"> 
      op.Grade 
    </column>
  </SpecialColumns>
</CustomColumns>
